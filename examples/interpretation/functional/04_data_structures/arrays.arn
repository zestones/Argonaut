// Example of arrays in Argonaut
type list : array[0: 5] of int;
var i : int;

// Declare and initialize an array of integers
var numbers : list;

numbers[0] := 1;
numbers[1] := 2;
numbers[2] := 3;
numbers[3] := 4;
numbers[4] := 5;

// Print the array elements
for (i := 0; i < 5; i++) {
    print("numbers[%d] = %d\n", i, numbers[i]);
}
