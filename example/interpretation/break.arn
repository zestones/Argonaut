PROG

var n : int;

proc main() {
    while(n > 0) {
        if (n % 2 == 0) {
            print("n: %d\n", n);
            break;
        }
        
        n--;
    }
}

n := 4;
main();

print("FOR LOOP\n");
for (n := 0; n < 5; n++) {
    if (n == 2) {
        break;
    }
    
    print("n = %d\n", n);
}